{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-underscore-dangle": 0,
		"no-prototype-builtins": 0,
		"class-methods-use-this": 0
    }
}
