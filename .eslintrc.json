{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-underscore-dangle": 0,
		"no-prototype-builtins": 0,
		"class-methods-use-this": 0,
		"no-unused-vars": 0,
		"no-empty-function": 0,
		"no-shadow": 0,
		"import/prefer-default-export": 0,
		"no-param-reassign": 0,
		"arrow-parens": 0,
		"no-useless-constructor": 0,
		"prefer-promise-reject-errors": 0,
		"no-useless-escape": 0
    }
}
