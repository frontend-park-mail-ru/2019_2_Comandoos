<div id="{{_id}}">
	<div class="tp-container tp-centered-container">
		<div class="tp-row tp-row_content_left">
			<div class="tp-col-8 tp-col_padding_0">
				<div class="card card_page-header card_page-header_fit">
					{{{ pageHeader }}}
				</div>
			</div>
		</div>
		<div class="tp-row tp-row_content_left">
			<div class="tp-col-8 tp-col_padding_0">
				<div class="card card_no-padding">
					{{#each jobs}}
						{{{this}}}
					{{/each}}

				</div>
				{{#loading}}
					<div>Загрузка...</div>
				{{/loading}}
			</div>
		</div>
	</div>
</div>
