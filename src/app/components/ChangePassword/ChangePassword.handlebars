<div class="tp-container" id="{{props.id}}">
	<div class="tp-row tp-row_content_left tp-underline">
		<h2>
			Пароль
		</h2>
	</div>
	<div class="tp-row tp-row_content_left">
		<div class="tp-col-6">
			<div class="tp-container" id="passwordChangeBlock">
				<form
						class="tp-form"
						id="passwordChangeForm"
						method="post"
						autocomplete="off"
				>
					{{{ data.currentPasswordField }}}
					{{{ data.newPasswordField }}}
					{{{ data.newPasswordConfirmField }}}
					{{{ data.submitBtn }}}
				</form>
			</div>
		</div>
	</div>
</div>
