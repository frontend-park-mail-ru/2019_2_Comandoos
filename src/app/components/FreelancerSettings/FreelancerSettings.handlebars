<div id="{{_id}}">
	<div class="tp-container">
		<div class="tp-row tp-row_content_stretch tp-underline">
			<h2>
				Контакты
			</h2>
		</div>
		<div class="tp-row tp-row_content_stretch">
			<div class="tp-col-12">
				<div class="tp-container">
					<form
						class="tp-form"
						id="contactsForm"
						method="post"
						autocomplete="off"
					>
						<div class="tp-col-8 tp-col_padding_0">
							{{{citySelect}}}
						</div>
						<div class="tp-col-8 tp-col_padding_0">
							{{{ addressField }}}
							<span class="tp-label-note">Мы серьезно относимся к соблюдению конфиденциальности.
												Только ваш город и страна будут доступны клиентам</span>
						</div>

						<div class="tp-col-8 tp-col_padding_0">
							{{{ phoneField }}}
						</div>

						<div class="tp-col-8 tp-col_padding_0">
							{{{ submitBtn }}}
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>

	<div class="tp-container">
		<div class="tp-row tp-row_content_stretch tp-underline">
			<h2>
				Мой профиль
			</h2>
		</div>
		<div class="tp-row tp-row_content_left">
			<div class="tp-col-6">
				<form
					class="tp-form"
					id="profileSettingsForm"
					method="post"
					autocomplete="off"
					novalidate="novalidate"
				>

					{{{ levelRadioGroup }}}

					{{{ submitBtn }}}

				</form>
			</div>
		</div>
	</div>

	<div class="tp-container">
		<div class="tp-row tp-row_content_stretch tp-row_align_center tp-underline">
			<h2>
				Выбор специализации и категорий услуг
			</h2>
			<a class="tp-row__right " href="#" target="_self">Изменить</a>
		</div>
	</div>
	<div class="tp-col-12">
		{{#each skills}}
			<label class="field-group__label">{{section}}</label>
			<div class="tp-row tp-row_content_left">
				{{#each categories}}
					{{{ this }}}
				{{/each}}
			</div>
		{{/each}}
	</div>
</div>
