<div id="{{_id}}">
	<div class="tp-container">
		<div class="tp-row tp-row_content_center">
			{{{ companyLogo }}}
		</div>
		<div class="tp-container">
			<form
				class="tp-form"
				id="companySettingsForm"
				method="post"
				autocomplete="off"
			>
				{{{ titleField }}}
				{{{ siteField }}}
				{{{ headerField }}}
				{{{ descriptionField }}}

				{{{ submitBtn }}}
			</form>
		</div>
	</div>
	<div class="tp-container">
		<div class="tp-row tp-row_content_left tp-underline">
			<h3>
				Контакты компании
			</h3>
		</div>
		<div class="tp-container">
			<form
				class="tp-form"
				id="companyContactsForm"
				method="post"
				autocomplete="off"
			>
				{{{ ownerField }}}
				{{{ citySelect }}}
				{{{ addressField }}}
				{{{ phoneField }}}

				{{{ submitBtn }}}
			</form>
		</div>
	</div>
</div>
