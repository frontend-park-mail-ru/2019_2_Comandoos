<div class="tp-container tp-centered-container" id="{{_id}}">
	<div class="tp-row tp-row_content_left">
		<div class="tp-col-8 tp-col_padding_0">
			<div class="card card_page-header card_page-header_fit">
				{{{ pageHeader }}}
			</div>
		</div>
	</div>
	<div class="tp-row tp-row_content_left">
		<div class="tp-col-8 card">
			<h1>{{title}}</h1>
			<form method="post" id="projectForm" autocomplete="off">
				{{{_jobTypeRadio}}}
				{{{textField}}}
				{{{descriptionField}}}
				{{{specialitySelect}}}
				{{{inputTags}}}
				{{{budgetField}}}

{{!--				{{#if isVacancy}}--}}
					{{{citySelect}}}
{{!--				{{/if}}--}}

				{{{levelRadioGroup}}}
				{{{submitBtn}}}
			</form>
		</div>
	</div>
</div>
